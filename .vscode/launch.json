{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "App.Server",
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/App/App.Server/App.Server.csproj"
    },
    {
      "command": "npm start --prefix App/App.Web",
      "name": "App.Web",
      "request": "launch",
      "type": "node-terminal"
    }
  ],
  "compounds": [
    {
      "name": "App.Server/App.Web",
      "configurations": [
        "App.Server",
        "App.Web"
      ],
      "stopAll": true
    }
  }
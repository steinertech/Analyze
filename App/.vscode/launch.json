{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "App.Server",
      // "type": "coreclr",
      // "request": "attach",
      // "processId": "${command:azureFunctions.pickProcess}"
      "type": "dotnet",
      "request": "launch",
      "projectPath": "${workspaceFolder}/App.Server/App.Server.csproj",
    },
    {
      "name": "App.Web",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/"
    },
  ],
  "compounds": [
    {
      "name": "App.Server/App.Web",
      "configurations": [
        "App.Server",
        "App.Web"
      ],
      "stopAll": true
    }
  ]
}
